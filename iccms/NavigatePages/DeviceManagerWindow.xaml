<Window x:Class="iccms.NavigatePages.DeviceManagerWindow" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:TreeItemModel="clr-namespace:DataInterface;assembly=DataInterface" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:iccms.NavigatePages" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        mc:Ignorable="d"
        Name="FrmDeviceAdvanceSetting" 
        Title="{Binding DeviceManagement}" 
        Width="870" 
        Height="600" 
        AllowsTransparency="True" 
        Background="{x:Null}" 
        Closed="FrmDeviceAdvanceSetting_Closed" 
        Closing="Window_Closing" 
        FontSize="14" 
        Icon="..\icon\Safari.ico" 
        KeyDown="FrmDeviceAdvanceSetting_KeyDown" 
        Loaded="Window_Loaded" 
        MouseLeftButtonDown="FrmDeviceAdvanceSetting_MouseLeftButtonDown" 
        MouseMove="FrmDeviceAdvanceSetting_MouseMove" 
        WindowStartupLocation="CenterScreen" 
        WindowState="Normal" 
        WindowStyle="None">

    <Border Width="Auto" Height="Auto" Background="{StaticResource WindowBrush}" BorderBrush="#FF05FF54" BorderThickness="1" CornerRadius="10,10,10,10">
        <Grid Margin="5 5 5 5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="220" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <!--  设备树列表  -->
            <Grid Grid.Row="0" Grid.Column="0" Background="{StaticResource DeviceTreeListBrush}">
                <Grid.RowDefinitions>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <TreeView Name="DeviceListTreeView" Background="Transparent">
                        <TreeView.ItemTemplate>
                            <HierarchicalDataTemplate DataType="{x:Type TreeItemModel:CheckBoxTreeModel}" ItemsSource="{Binding Children}">
                                <StackPanel x:Name="staTree" Orientation="Horizontal">
                                    <TextBlock FontSize="14" PreviewMouseLeftButtonUp="btnDevieName_PreviewMouseLeftButtonUp" Tag="{Binding Children}" ToolTip="{Binding ToolTip}">
                                        <StackPanel Orientation="Horizontal">
                                            <Image Width="16" Height="16" VerticalAlignment="Center" PreviewMouseLeftButtonUp="btnDevieName_PreviewMouseLeftButtonUp" Source="{Binding Icon}" />
                                            <TextBlock Name="btnDevieName" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Background="Transparent" PreviewMouseLeftButtonUp="btnDevieName_PreviewMouseLeftButtonUp" Text="{Binding Name}" />
                                        </StackPanel>
                                    </TextBlock>
                                </StackPanel>
                            </HierarchicalDataTemplate>
                        </TreeView.ItemTemplate>
                    </TreeView>
                </Grid>
            </Grid>

            <!--  各设备功能选项卡  -->
            <Grid Name="DeviceOperationPage" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2" Background="Transparent" Visibility="Visible">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="50" />
                </Grid.RowDefinitions>

                <!--  总控制标签  -->
                <TabControl Name="tabDeviceModelControl" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Background="Transparent">
                    <TabControl.ItemContainerStyle>
                        <Style>
                            <Setter Property="TabItem.Visibility" Value="Collapsed" />
                        </Style>
                    </TabControl.ItemContainerStyle>

                    <!--  LTE设备功能选项卡  -->
                    <TabItem Name="tabLTEModel">
                        <TabControl x:Name="tabControlSeting" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Background="Transparent" MouseLeftButtonDown="tabControlSeting_MouseLeftButtonDown" MouseMove="tabControlSeting_MouseMove" Visibility="Visible">

                            <!--  设备信息设置  -->
                            <TabItem Name="tmDeviceInfo" Header="{Binding DeviceInfo}" IsEnabled="{Binding OnLineControl}">
                                <Grid Grid.Row="0" Grid.Column="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="70" />
                                        <ColumnDefinition Width="65" />
                                        <ColumnDefinition Width="75" />
                                        <ColumnDefinition Width="75" />
                                        <ColumnDefinition Width="75" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="30" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <Label x:Name="lblDomainName" Grid.Row="0" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding DomainNameCaption}" />
                                    <TextBox x:Name="txtDomainName" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="5" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding DomainFullPathName}" />

                                    <Label x:Name="lblStation" Grid.Row="1" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding StationCaption}" />
                                    <TextBox x:Name="txtStation" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Station}" />

                                    <Label x:Name="lblDeviceName" Grid.Row="1" Grid.Column="3" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding DeviceNameCaption}" />
                                    <TextBox x:Name="txtDeviceName" Grid.Row="1" Grid.Column="4" Grid.ColumnSpan="2" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding DeviceName}" />

                                    <Label x:Name="lblDeviceMode" Grid.Row="2" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding ModeCaption}" />

                                    <ComboBox Name="cbxDeviceMode" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Style="{StaticResource ComboBoxStyle}" Text="{Binding DeviceMode}">
                                        <ComboBoxItem Content="LTE-FDD" />
                                        <ComboBoxItem Content="LTE-TDD" />
                                        <ComboBoxItem Content="WCDMA" />
                                        <ComboBoxItem Content="CDMA" />
                                        <ComboBoxItem Content="TD-SCDMA" />
                                        <ComboBoxItem Content="GSM" />
                                        <ComboBoxItem Content="GSM-V2" />
                                    </ComboBox>

                                    <Label x:Name="lblIPMode" Grid.Row="3" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding IpCaption}" />

                                    <RadioButton x:Name="rbStaticIPMode" Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" VerticalContentAlignment="Center" Cursor="Hand">
                                        <RadioButton.Template>
                                            <ControlTemplate>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <RadioButton Name="rbStaticIPMode" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Cursor="Hand" GroupName="IPSetting" IsChecked="{Binding StaticIPMode}" />
                                                    <TextBlock Name="txtStaticIPMode" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Text="固定IP" />
                                                </Grid>
                                            </ControlTemplate>
                                        </RadioButton.Template>
                                    </RadioButton>

                                    <TextBox x:Name="txtIPAddr" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding IpAddr}" />
                                    <Label x:Name="lblPort" Grid.Row="3" Grid.Column="4" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Content=":" />
                                    <TextBox x:Name="txtPort" Grid.Row="3" Grid.Column="4" Height="30" Margin="15,0,0,0" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Port}" />

                                    <RadioButton x:Name="rbDynamicIPMode" Grid.Row="4" Grid.Column="1" VerticalAlignment="Center" VerticalContentAlignment="Center" Cursor="Hand">
                                        <RadioButton.Template>
                                            <ControlTemplate>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <RadioButton Name="rbDynamicIPMode" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Cursor="Hand" GroupName="IPSetting" IsChecked="{Binding DynamicIPMode}" />
                                                    <TextBlock Name="txtDynamicIPMode" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Text="动态IP" />
                                                </Grid>
                                            </ControlTemplate>
                                        </RadioButton.Template>
                                    </RadioButton>

                                    <Grid Grid.Row="3" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40" />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Label Name="lblNetMask" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="掩码" />
                                        <TextBox x:Name="txtNetMask" Grid.Row="0" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding NetMask}" />
                                    </Grid>

                                    <Label x:Name="lblSN" Grid.Row="5" Grid.Column="0" Height="25" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding SerialNumberCaption}" />
                                    <TextBox x:Name="txtSN" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" MaxLength="16" Text="{Binding SN}" />

                                    <Label x:Name="lblDeviceIdentificationMode" Grid.Row="6" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding DeviceDistinguishCaption}" />

                                    <ComboBox Name="cbxDeviceIdentificationMode" Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="3" Height="30" VerticalContentAlignment="Center" Style="{StaticResource ComboBoxStyle}" Text="{Binding DeviceIdentificationMode}">
                                        <ComboBoxItem Content="设备全名" />
                                        <ComboBoxItem Content="IP" />
                                        <ComboBoxItem Content="系列号" />
                                    </ComboBox>

                                    <Grid Grid.Row="7" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Button x:Name="btnAdd" Grid.Row="7" Grid.Column="0" Width="75" Height="30" VerticalAlignment="Center" Click="btnAdd_Click" Content="{Binding Add}" />
                                        <Button x:Name="btnDelete" Grid.Row="7" Grid.Column="1" Width="75" Height="30" VerticalAlignment="Center" Click="btnDelete_Click" Content="{Binding Delete}" />
                                        <Button x:Name="btnUpdate" Grid.Row="7" Grid.Column="2" Width="75" Height="30" VerticalAlignment="Center" Click="btnUpdate_Click" Content="{Binding Update}" />
                                    </Grid>
                                </Grid>
                            </TabItem>

                            <!--  小区信息  -->
                            <TabItem Name="tmCellNeighInfo" Header="{Binding CellNeighInfo}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="270" />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <GroupBox BorderThickness="0.6" Header="小区信息" Style="{StaticResource GroupboxWithoutWhiteBorder}">
                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="75" />
                                                <ColumnDefinition Width="105" />
                                                <ColumnDefinition Width="125" />
                                                <ColumnDefinition Width="105" />
                                                <ColumnDefinition Width="75" />
                                                <ColumnDefinition Width="105" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="40" />
                                            </Grid.RowDefinitions>
                                            <Label Name="lblPLMN" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding PlmnCaption}" />
                                            <TextBox Name="txtPLMN" Grid.Row="0" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" KeyUp="txtPLMN_KeyUp" Text="{Binding PLMN}" />

                                            <Label Name="lblFreqPoint" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding FreqPointCaption}" />
                                            <TextBox Name="txtFreqPoint" Grid.Row="1" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding FrequencyPoint}" />

                                            <Label Name="lblBandWidth" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding BandWidthCaption}" />
                                            <TextBox Name="txtBandWidth" Grid.Row="2" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Cursor="Hand" MouseDoubleClick="txtBandWidth_MouseDoubleClick" Text="{Binding BandWidth}" ToolTip="双击选择带宽" />
                                            <Label Name="lblBandWidthUnit" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="M" />

                                            <Label Name="lblPowerDown" Grid.Row="3" Grid.Column="0" VerticalAlignment="Center" HorizontalContentAlignment="Right" Content="{Binding PowerDownCaption}" />
                                            <TextBox Name="txtPowerDown" Grid.Row="3" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding PowerAttenuation}" />
                                            <Label Name="lblDB" Grid.Row="3" Grid.Column="2" Width="28" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="dB" />

                                            <Label Name="lblFreqMethod" Grid.Row="4" Grid.Column="0" Height="30" VerticalAlignment="Center" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Content="{Binding FreqMethodCaption}" />

                                            <Grid Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="75" />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <CheckBox Name="rdbFreqAuto" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="rdbFreqAuto_Click" Content="自动" IsChecked="{Binding FrequencyChioceModeAuto}" />
                                                <CheckBox Name="rdbFreqManul" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="rdbFreqManul_Click" Content="手动" IsChecked="{Binding FrequencyChioceModeManul}" />
                                            </Grid>

                                            <Label Name="lblOperatorName" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding OperatorNameCaption}" />
                                            <TextBox Name="txtOperatorName" Grid.Row="0" Grid.Column="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Text="{Binding Operators}" />

                                            <Label Name="lblDisturbCode" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding DisturbCodeCaption}" />
                                            <TextBox Name="txtDisturbCode" Grid.Row="1" Grid.Column="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Scrambler}" />

                                            <Label Name="lblTACorLAC" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding TACorLACCaption}" />
                                            <TextBox Name="txtTACorLAC" Grid.Row="2" Grid.Column="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding TacLac}" />

                                            <Label Name="lblCycle" Grid.Row="3" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding CycleCaption}" />
                                            <TextBox Name="txtCycle" Grid.Row="3" Grid.Column="3" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Period}" />
                                            <Label Name="lblSecond" Grid.Row="3" Grid.Column="4" HorizontalAlignment="Left" VerticalAlignment="Center" Content="{Binding lblSecond}" />

                                            <Label Name="lblCellID" Grid.Row="0" Grid.Column="4" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="小区ID" />
                                            <TextBox Name="txtCellID" Grid.Row="0" Grid.Column="5" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding CellID}" />

                                            <Label Name="lblStartupMode" Grid.Row="4" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="{Binding StartupModeCaption}" />

                                            <Grid Grid.Row="4" Grid.Column="3" Grid.ColumnSpan="2">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="75" />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <CheckBox Name="rdbRebootModeAuto" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="rdbRebootModeAuto_Click" Content="自动" IsChecked="{Binding RebootModeAuto}" />
                                                <CheckBox Name="rdbRebootModeManul" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="rdbRebootModeManul_Click" Content="手动" IsChecked="{Binding RebootModeManul}" />
                                            </Grid>

                                            <Button Name="btnCellNeighUpdate" Grid.Row="5" Grid.Column="5" Width="105" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="btnCellNeighUpdate_Click" Content="{Binding DeviceUpdateCaption}" />
                                        </Grid>
                                    </GroupBox>

                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <!--  等效PLMN  -->
                                        <GroupBox Name="grbMorePLMN" Grid.Row="0" Grid.Column="0" BorderThickness="0.6" Header="等效PLMN信息" Style="{StaticResource GroupboxWithoutWhiteBorder}">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition Height="35" />
                                                </Grid.RowDefinitions>
                                                <DataGrid Name="MorePLMNSListDataGrid" Grid.Row="0" Grid.Column="0" AutoGenerateColumns="False" Background="Transparent" CanUserAddRows="True" IsReadOnly="False">
                                                    <DataGrid.Columns>
                                                        <DataGridTextColumn Width="125*" Binding="{Binding Path=PLMNS}">
                                                            <DataGridTextColumn.Header>
                                                                <TextBlock Name="txtMorePLMNSList" Text="{Binding MorePLMNSListCaption, Mode=TwoWay}" />
                                                            </DataGridTextColumn.Header>
                                                        </DataGridTextColumn>
                                                    </DataGrid.Columns>

                                                    <DataGrid.ContextMenu>
                                                        <ContextMenu>
                                                            <MenuItem Name="miMultiPLMNListClear" Click="miMultiPLMNListClear_Click" Header="清空">
                                                                <MenuItem.Icon>
                                                                    <Image Width="16" Height="16" Source="..\Icon\edit_remove.png" />
                                                                </MenuItem.Icon>
                                                            </MenuItem>
                                                        </ContextMenu>
                                                    </DataGrid.ContextMenu>

                                                    <DataGrid.ColumnHeaderStyle>
                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                            <Setter Property="Background" Value="DodgerBlue" />
                                                            <Setter Property="Foreground" Value="White" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                        </Style>
                                                    </DataGrid.ColumnHeaderStyle>

                                                    <DataGrid.RowStyle>
                                                        <Style TargetType="DataGridRow">
                                                            <Setter Property="Height" Value="25" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="VerticalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="Black" />
                                                        </Style>
                                                    </DataGrid.RowStyle>

                                                    <DataGrid.Resources>
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Transparent" />
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightTextBrushKey}" Color="Black" />
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="White" />
                                                    </DataGrid.Resources>

                                                    <DataGrid.RowBackground>
                                                        <ImageBrush ImageSource="{x:Null}" Opacity="1" />
                                                    </DataGrid.RowBackground>
                                                </DataGrid>
                                                <Button Name="btnOtherPlmnSetting" Grid.Row="1" Width="105" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="btnOtherPlmnSetting_Click" Content="{Binding OtherPlmnSettingCaption}" />
                                            </Grid>
                                        </GroupBox>

                                        <!--  周期频点  -->
                                        <GroupBox Name="grbPeriodFrea" Grid.Row="0" Grid.Column="1" BorderThickness="0.6" Header="周期频点" Style="{StaticResource GroupboxWithoutWhiteBorder}">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition Height="30" />
                                                    <RowDefinition Height="35" />
                                                </Grid.RowDefinitions>
                                                <DataGrid Name="PeriodFreqDataGrid" Grid.Row="0" Grid.Column="0" AutoGenerateColumns="False" Background="Transparent" CanUserAddRows="True" IsReadOnly="False">
                                                    <DataGrid.Columns>
                                                        <DataGridTextColumn Width="125*" Binding="{Binding Path=PerierFreq}">
                                                            <DataGridTextColumn.Header>
                                                                <TextBlock Name="txtPerierFreqList" Text="{Binding PerierFreqListCaption, Mode=TwoWay}" />
                                                            </DataGridTextColumn.Header>
                                                        </DataGridTextColumn>
                                                    </DataGrid.Columns>

                                                    <DataGrid.ContextMenu>
                                                        <ContextMenu>
                                                            <MenuItem Name="miFreqListClear" Click="miFreqListClear_Click" Header="清空">
                                                                <MenuItem.Icon>
                                                                    <Image Width="16" Height="16" Source="..\Icon\edit_remove.png" />
                                                                </MenuItem.Icon>
                                                            </MenuItem>
                                                        </ContextMenu>
                                                    </DataGrid.ContextMenu>

                                                    <DataGrid.ColumnHeaderStyle>
                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                            <Setter Property="Background" Value="DodgerBlue" />
                                                            <Setter Property="Foreground" Value="White" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                        </Style>
                                                    </DataGrid.ColumnHeaderStyle>

                                                    <DataGrid.RowStyle>
                                                        <Style TargetType="DataGridRow">
                                                            <Setter Property="Height" Value="25" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="VerticalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="Black" />
                                                        </Style>
                                                    </DataGrid.RowStyle>

                                                    <DataGrid.Resources>
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Transparent" />
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightTextBrushKey}" Color="Black" />
                                                        <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="White" />
                                                    </DataGrid.Resources>

                                                    <DataGrid.RowBackground>
                                                        <ImageBrush ImageSource="{x:Null}" Opacity="1" />
                                                    </DataGrid.RowBackground>
                                                </DataGrid>
                                                <Grid Grid.Row="1" Grid.Column="0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="40" />
                                                        <ColumnDefinition />
                                                        <ColumnDefinition Width="30" />
                                                    </Grid.ColumnDefinitions>
                                                    <Label Name="lblPeriodFreqCaption" Grid.Column="0" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="周期" />
                                                    <TextBox Name="txtPeriodFreq" Grid.Row="0" Grid.Column="1" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Cycle}" />
                                                    <Label Name="lblPeriodFreaUnit" Grid.Row="0" Grid.Column="2" Height="30" VerticalAlignment="Center" VerticalContentAlignment="Center" Content="秒" />
                                                </Grid>
                                                <Button Name="btnPeriodFreqSetting" Grid.Row="2" Grid.Column="0" Width="105" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="btnPeriodFreqSetting_Click" Content="{Binding PeriodFreqSettingCaption}" />
                                            </Grid>
                                        </GroupBox>

                                        <!--  同步状态信息  -->
                                        <GroupBox Name="grbLTESYNCInfo" Grid.Row="0" Grid.Column="2" BorderThickness="0.6" Header="同步状态信息" Style="{StaticResource GroupboxWithoutWhiteBorder}">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="60" />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition Width="10" />
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="状态" />
                                                <Label Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="同步源" />
                                                <Label Grid.Row="2" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="频点" />
                                                <Label Grid.Row="3" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="PCI" />

                                                <TextBox x:Name="txtLTEStatus" Grid.Row="0" Grid.Column="1" Width="120" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Status}" />
                                                <TextBox x:Name="txtLTESource" Grid.Row="1" Grid.Column="1" Width="120" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Source}" />
                                                <TextBox x:Name="txtLTEEuarfcn" Grid.Row="2" Grid.Column="1" Width="120" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding Euarfcn}" />
                                                <TextBox x:Name="txtLTEPCI" Grid.Row="3" Grid.Column="1" Width="120" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center" Text="{Binding PCI}" />

                                                <Button x:Name="btnLTESYNCInfo" Grid.Row="4" Grid.Column="1" Width="75" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="btnLTESYNCInfo_Click" Content="查询" />
                                            </Grid>
                                        </GroupBox>
                                    </Grid>

                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4" />
                                            <ColumnDefinition />
