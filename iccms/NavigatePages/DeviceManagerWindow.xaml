                                    <TextBox x:Name="txtGSMV3CacheMax" Grid.Row="3" Grid.Column="3" Width="150" Height="30" HorizontalAlignment="Left" VerticalContentAlignment="Center" Text="{Binding CacheMax}" />

                                    <Label x:Name="lblGSMV3DataFormat" Grid.Row="4" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding DataFormat}" />
                                    <TextBox x:Name="txtGSMV3DataFormat" Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" Width="510" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding DataFormat}" />

                                    <Label x:Name="lblGSMV3NameFormat" Grid.Row="5" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding NameFormat}" />
                                    <TextBox x:Name="txtGSMV3NameFormat" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Width="510" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding NameFormat}" />

                                    <Label x:Name="lblGSMV3URLorIP" Grid.Row="6" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding URLorIP}" />
                                    <TextBox x:Name="txtGSMV3URLorIP" Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="3" Width="510" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding URLorIP}" />

                                    <Label x:Name="lblGSMV3AddInfo" Grid.Row="7" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding AddInfo}" />
                                    <TextBox x:Name="txtGSMV3AddInfo" Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="3" Width="510" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding AddInfo}" />

                                    <Button x:Name="btnGSMV3SelectUpLoad" Grid.Row="8" Grid.Column="1" Width="85" Height="30" Content="{Binding SelectUpLoad}" />
                                    <Button x:Name="btnGSMV3SetUpLoad" Grid.Row="8" Grid.Column="2" Width="85" Height="30" Content="{Binding SetUpLoad}" />
                                </Grid>
                            </TabItem>

                        </TabControl>
                    </TabItem>

                    <!--CDMA-V3-->

                </TabControl>

                <!--  关闭  -->
                <Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition Width="15" />
                    </Grid.ColumnDefinitions>
                    <Button Name="btnClose" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Width="105" Height="30" Margin="0,0,10.5,0" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Click="btnClose_Click" Content="关闭" />
                </Grid>
            </Grid>
        </Grid>
    </Border>
</Window>
