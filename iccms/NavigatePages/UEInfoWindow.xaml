<Page x:Class="iccms.NavigatePages.UEInfoWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:iccms.NavigatePages"
      xmlns:DataRowBackGround="clr-namespace:iccms.NavigatePages"     
      mc:Ignorable="d" 
      d:DesignHeight="800" 
      d:DesignWidth="1024" 
      MinWidth="300"
      FontSize="12"
      Background="{x:Null}"
      Title="捕号数据信息" 
      Loaded="Page_Loaded">

    <Border Name="UEInfoBox" MinWidth="300">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="3"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>

            <!--捕号上报窗口-->
            <Border Name="ScannerWindow" Grid.Row="{Binding ScannerWndRow}" Grid.Column="{Binding ScannerWndColumn}" Grid.RowSpan="{Binding ScannerWndRowSpan}" Grid.ColumnSpan="{Binding ScannerWndColumnSpan}" Visibility="{Binding ScannerWndVisibility}" Background="{StaticResource ScannerWindowBrush}" Margin="5,5,5,2.5">
                <Grid Height="Auto" Width="Auto" Visibility="Visible">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="60"></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>

                        <GroupBox Name="ScannerFillterArea" Grid.Row="0" Style="{StaticResource GroupboxWithoutWhiteBorder}" Height="60" VerticalAlignment="Center">
                            <Grid VerticalAlignment="Center" Margin="0 10 0 0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="38"></ColumnDefinition>
                                    <ColumnDefinition Width="115"></ColumnDefinition>
                                    <ColumnDefinition Width="60"></ColumnDefinition>
                                    <ColumnDefinition Width="105"></ColumnDefinition>
                                    <ColumnDefinition Width="105"></ColumnDefinition>
                                    <ColumnDefinition Width="25"></ColumnDefinition>
                                    <ColumnDefinition Width="105"></ColumnDefinition>
                                    <ColumnDefinition Width="105"></ColumnDefinition>
                                    <ColumnDefinition Width="35"></ColumnDefinition>
                                    <ColumnDefinition Width="115"></ColumnDefinition>
                                    <ColumnDefinition Width="58"></ColumnDefinition>
                                    <ColumnDefinition Width="70"></ColumnDefinition>
                                    <ColumnDefinition Width="85"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35"></RowDefinition>
                                    <RowDefinition Height="5"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Label  Name="lblIMSI" Grid.Column="0" Grid.Row="0" Content="{Binding IMSILable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                <TextBox Name="txtIMSI" Grid.Column="1" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" MaxLength="15" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" TextChanged="txtIMSI_TextChanged"></TextBox>

                                <Label  Name="lblcmdStartTime" Grid.Column="2" Grid.Row="0" Content="{Binding TimeStartLable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                <DatePicker Name="dpkcmdStartDate" Grid.Column="3" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" Background="Transparent" SelectedDateChanged="dpkcmdStartTime_SelectedDateChanged"></DatePicker>

                                <Grid Grid.Column="4" Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="13"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="13"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Name="txtStartHour" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtStartHour_TextChanged"></TextBox>
                                    <Label Name="lblStartHour" Grid.Column="1" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                    <TextBox Name="txtStartMinute" Grid.Column="2" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtStartMinute_TextChanged"></TextBox>
                                    <Label Name="lblStartMinute" Grid.Column="3" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                    <TextBox Name="txtStartSecond" Grid.Column="4" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtStartSecond_TextChanged"></TextBox>
                                </Grid>

                                <Label Name="lblTo" Content="{Binding ToLable}" Grid.Column="5" Grid.Row="0" Width="28" VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>

                                <DatePicker Name="dpkcmdEndDate" Grid.Column="6" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" Background="Transparent" SelectedDateChanged="dpkcmdEndTime_SelectedDateChanged"></DatePicker>

                                <Grid Grid.Column="7" Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="13"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="13"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Name="txtEndHour" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtEndHour_TextChanged"></TextBox>
                                    <Label Name="lblEndHour" Grid.Column="1" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                    <TextBox Name="txtEndMinute" Grid.Column="2" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtEndMinute_TextChanged"></TextBox>
                                    <Label Name="lblEndMinute" Grid.Column="3" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                    <TextBox Name="txtEndSecond" Grid.Column="4" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtEndSecond_TextChanged"></TextBox>
                                </Grid>

                                <Label Name="lblDeviceName" Grid.Column="8" Grid.Row="0" Content="{Binding DeviceNameLable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                <TextBox Name="txtDeviceName" Grid.Column="9" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" TextChanged="txtDeviceName_TextChanged"></TextBox>

                                <Label Name="lblUerType" Grid.Column="10" Grid.Row="0" Content="{Binding UserTypeLable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                <ComboBox Name="cbbUserType" Grid.Column="11" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" Width="70" Style="{StaticResource ComboBoxStyle}" IsReadOnly="True" IsEditable="False" SelectionChanged="cbbUerType_SelectionChanged">
                                    <ComboBoxItem Content="全部"></ComboBoxItem>
                                    <ComboBoxItem Content="普通用户"></ComboBoxItem>
                                    <ComboBoxItem Content="白名单"></ComboBoxItem>
                                    <ComboBoxItem Content="黑名单"></ComboBoxItem>
                                </ComboBox>

                                <!--捕号过虑-->
                                <Button Name="btnSearch" Grid.Column="12" Grid.Row="0" Grid.RowSpan="1" Height="30" Width="75" Background="Transparent" Cursor="Hand" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center" Click="btnSearch_Click" MouseEnter="btnSearch_MouseEnter" MouseLeave="btnSearch_MouseLeave">
                                    <Button.Template>
                                        <ControlTemplate>
                                            <Border BorderBrush="DodgerBlue" BorderThickness="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="32"></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <Image Grid.Column="0" Source="..\Icon\search.png" Width="24" Height="24" VerticalAlignment="Center"></Image>
                                                    <Label Grid.Column="1" Content="{Binding SearchLable}" VerticalAlignment="Center" VerticalContentAlignment="Center" FontSize="14" Foreground="{StaticResource SearchBarCaption}"></Label>
                                                </Grid>
                                            </Border>
                                        </ControlTemplate>
                                    </Button.Template>
                                </Button>
                            </Grid>
                        </GroupBox>

                        <!--捕号窗口-->
                        <DataGrid Name="ScannerInfoDataGrid" Grid.Row="2" Grid.Column="0" Width="Auto" Background="Transparent" AutoGenerateColumns="False" IsReadOnly="True" ScrollViewer.CanContentScroll="True" VirtualizingStackPanel.VirtualizationMode="Recycling" VirtualizingStackPanel.IsVirtualizing ="True" SelectedCellsChanged="ScannerInfoDataGrid_SelectedCellsChanged" ToolTip="双击显示选中行的详细信息" PreviewMouseDoubleClick="ScannerInfoDataGrid_PreviewMouseDoubleClick">
                            <DataGrid.Resources>
                                <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Transparent"/>
                                <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightTextBrushKey}" Color="Black"/>
                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="White"/>
                                <DataRowBackGround:BGConverter x:Key="bgconverter"/>
                            </DataGrid.Resources>

                            <DataGrid.Columns>
                                <DataGridTextColumn Width="50" Binding="{Binding Path=ID,Mode=TwoWay}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockID" MouseLeftButtonDown="gdBlockID_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockID" Grid.Column="0" Text ="{Binding IDHead}" />
                                            <Image Name="ImgBlockID" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="140" Binding="{Binding Path=IMSI}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockIMSI" MouseLeftButtonDown="gdBlockIMSI_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockIMSI" Text ="{Binding IMSIHead}" />
                                            <Image Name="ImgBlockIMSI" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="155" Binding="{Binding Path=DTime}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockDTime" MouseLeftButtonDown="gdBlockDTime_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockDTime" Text ="{Binding DTimeHead}" />
                                            <Image Name="ImgBlockDTime" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="75" Binding="{Binding Path=UserType}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockUserType" MouseLeftButtonDown="gdBlockUserType_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockUserType" Text ="{Binding UserTypeHead}" />
                                            <Image Name="ImgBlockUserType" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="140" Binding="{Binding Path=TMSI}" Visibility="Hidden">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockTMSI" MouseLeftButtonDown="gdBlockTMSI_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockTMSI" Text ="{Binding TMSIHead}" />
                                            <Image Name="ImgBlockTMSI" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="140" Binding="{Binding Path=IMEI}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockIMEI" MouseLeftButtonDown="gdBlockIMEI_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockIMEI" Text ="{Binding IMEIHead}" />
                                            <Image Name="ImgBlockIMEI" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="65" Binding="{Binding Path=Intensity}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockIntensity" MouseLeftButtonDown="gdBlockIntensity_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockIntensity" Text ="{Binding IntensityHead}" />
                                            <Image Name="ImgBlockIntensity" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="105" Binding="{Binding Path=Operators}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockOperators" MouseLeftButtonDown="gdBlockOperators_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockOperators" Text ="{Binding OperatorsHead}" />
                                            <Image Name="ImgBlockOperators" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="155" Binding="{Binding Path=DomainName}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockDistrict" MouseLeftButtonDown="gdBlockDistrict_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockDistrict" Text ="{Binding DistrictHead}" />
                                            <Image Name="ImgBlockDistrict" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="300" Binding="{Binding Path=DeviceName}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockDeviceName" MouseLeftButtonDown="gdBlockDeviceName_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockDeviceName" Text ="{Binding DeviceNameHead}" />
                                            <Image Name="ImgBlockDeviceName" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="185" Binding="{Binding Path=Des}">
                                    <DataGridTextColumn.Header>
                                        <Grid Name="gdBlockAliasName" MouseLeftButtonDown="gdBlockAliasName_MouseLeftButtonDown">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                <ColumnDefinition Width="16"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Name="txtBlockAliasName" Text ="{Binding AliasNameHead}" />
                                            <Image Name="ImgBlockAliasName" Grid.Column="1" Source="..\Icon\1downarrow1.png" Width="12" Height="12"></Image>
                                        </Grid>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                            </DataGrid.Columns>

                            <DataGrid.ColumnHeaderStyle>
                                <Style TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Height" Value="25" />
                                    <Setter Property="Background" Value="#FF215E21"/>
                                    <Setter Property="Foreground" Value="White"/>
                                    <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                </Style>
                            </DataGrid.ColumnHeaderStyle>

                            <DataGrid.RowStyle>
                                <Style TargetType="DataGridRow">
                                    <Setter Property="Height" Value="25" />
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                                    <Setter Property="Foreground" Value="Black"/>
                                    <Setter Property="Background" Value="{Binding Path=UserType, Converter={StaticResource bgconverter}}"/>
                                </Style>
                            </DataGrid.RowStyle>

                            <DataGrid.ContextMenu>
                                <ContextMenu Name="pmMenuAction" FontSize="14">

                                    <ContextMenu.Background>
                                        <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                    </ContextMenu.Background>

                                    <MenuItem Name="mmScannerDataRefresh" Header="刷新" Width="150" Height="30" Foreground="White" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Cursor="Hand" Click="mmScannerDataRefresh_Click">
                                        <MenuItem.Icon>
                                            <Image Source="..\Icon\quick_restart.png" Width="16" Height="16"></Image>
                                        </MenuItem.Icon>
                                        <MenuItem.Background>
                                            <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                        </MenuItem.Background>
                                    </MenuItem>
                                    <MenuItem Name="mmScannerDataClean" Header="清屏" Width="150" Height="30" Foreground="White" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Cursor="Hand" Click="mmScannerDataClean_Click">
                                        <MenuItem.Icon>
                                            <Image Source="..\Icon\db_remove.png" Width="16" Height="16"></Image>
                                        </MenuItem.Icon>
                                        <MenuItem.Background>
                                            <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                        </MenuItem.Background>
                                    </MenuItem>
                                    <MenuItem Name="mmScannerDataCopy" Header="复制" Width="150" Height="30" Foreground="White" FontSize="14" VerticalContentAlignment="Center" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" Command="" Click="mmScannerDataCopy_Click">
                                        <MenuItem.Icon>
                                            <Image Source="..\Icon\cdcopy.png" Width="16" Height="16"/>
                                        </MenuItem.Icon>
                                        <MenuItem.Background>
                                            <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                        </MenuItem.Background>
                                    </MenuItem>
                                    <MenuItem Name="mmScannerDataExport" Header="导出" Width="150" Height="30" Foreground="White" FontSize="14" VerticalContentAlignment="Center" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" Command="" Click="mmScannerDataExport_Click">
                                        <MenuItem.Icon>
                                            <Image Source="..\Icon\Export.ico" Width="16" Height="16"></Image>
                                        </MenuItem.Icon>
                                        <MenuItem.Background>
                                            <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                        </MenuItem.Background>
                                    </MenuItem>

                                    <MenuItem Name="mmScannerDataStopRefresh" Header="停止刷新" Width="150" Height="30" Foreground="White" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Cursor="Hand" Click="mmScannerDataStopRefresh_Click">
                                        <MenuItem.Icon>
                                            <Image Source="..\Icon\agt_resume.png" Width="16" Height="16"></Image>
                                        </MenuItem.Icon>
                                        <MenuItem.Background>
                                            <ImageBrush ImageSource="..\BackGround\timg.png" Opacity="0.3"></ImageBrush>
                                        </MenuItem.Background>
                                    </MenuItem>
                                </ContextMenu>
                            </DataGrid.ContextMenu>
                        </DataGrid>
                    </Grid>
                </Grid>
            </Border>

            <!--上下移动-->
            <GridSplitter Name="ScannerWindowLine" Grid.Row="1" Grid.Column="0" Height="3" Visibility="{Binding ScannerWndLineVisibility}" HorizontalAlignment="Stretch" Background="{StaticResource GridSplitterStyle}" Margin="5,0,5,0"></GridSplitter>

            <!--测量报告/通话记录/短信记录上报窗口-->
            <Border Name="FunctionWindowsArea" Grid.Row="{Binding FunctionWndAreaRow}" Grid.Column="{Binding FunctionWndAreaColumn}" Grid.RowSpan="{Binding FunctionWndAreaRowSpan}" Grid.ColumnSpan="{Binding FunctionWndAreaColumnSpan}" Visibility="{Binding FunctionWndsAreaVisibility}" Background="{StaticResource BlackListWindowBrush}" Margin="5,2.5,5,5">
                <Grid  Name="DataReportModuleBox" Height="Auto" Width="Auto" Visibility="Visible">
                    <TabControl Name="TabControlBox" Background="Transparent" SelectedIndex="{Binding PageIndex}" SelectionChanged="TabControlBox_SelectionChanged">
                        <!--测量报告数据表-->
                        <TabItem Name="tabBlackReportDataView" Header="测量报告" Visibility="{Binding MeasurementReportItem}">
                            <Grid Height="Auto" Width="Auto" Visibility="Visible">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60"></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>

                                    <GroupBox Name="MeasurementSearchBar" Grid.Row="0" Style="{StaticResource GroupboxWithoutWhiteBorder}" Height="60" VerticalAlignment="Center">
                                        <Grid VerticalAlignment="Center" Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="38"></ColumnDefinition>
                                                <ColumnDefinition Width="105"></ColumnDefinition>
                                                <ColumnDefinition Width="60"></ColumnDefinition>
                                                <ColumnDefinition Width="105"></ColumnDefinition>
                                                <ColumnDefinition Width="105"></ColumnDefinition>
                                                <ColumnDefinition Width="25"></ColumnDefinition>
                                                <ColumnDefinition Width="105"></ColumnDefinition>
                                                <ColumnDefinition Width="105"></ColumnDefinition>
                                                <ColumnDefinition Width="35"></ColumnDefinition>
                                                <ColumnDefinition Width="115"></ColumnDefinition>
                                                <ColumnDefinition Width="60"></ColumnDefinition>
                                                <ColumnDefinition Width="70"></ColumnDefinition>
                                                <ColumnDefinition Width="85"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="35"></RowDefinition>
                                                <RowDefinition Height="5"></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Label  Name="lblBLIMSI" Grid.Column="0" Grid.Row="0" Content="{Binding IMSILable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                            <TextBox Name="txtBLIMSI" Grid.Column="1" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" TextChanged="txtBLIMSI_TextChanged" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}"></TextBox>

                                            <Label  Name="lblBLcmdStartTime" Grid.Column="2" Grid.Row="0" Content="{Binding TimeStartLable}" HorizontalAlignment="Right" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>

                                            <DatePicker Name="dpkBLcmdStartDate" Grid.Column="3" Grid.Row="0" VerticalAlignment="Center" VerticalContentAlignment="Center"  Height="30" Background="Transparent" SelectedDateChanged="dpkBLcmdStartTime_SelectedDateChanged"></DatePicker>

                                            <Grid Grid.Column="4" Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition Width="13"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition Width="13"></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <TextBox Name="txtBLStartHour" Grid.Column="0" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtBLStartHour_TextChanged"></TextBox>
                                                <Label Name="lblBLStartHour" Grid.Column="1" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                                <TextBox Name="txtBLStartMinute" Grid.Column="2" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtBLStartMinute_TextChanged"></TextBox>
                                                <Label Name="lblBLStartMinute" Grid.Column="3" Content=":" FontSize="14" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>
                                                <TextBox Name="txtBLStartSecond" Grid.Column="4" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="{StaticResource SearchBarText}" Background="{StaticResource SearchBarEdit}" Height="19" Width="25" MaxLength="2" TextChanged="txtBLStartSecond_TextChanged"></TextBox>
                                            </Grid>

                                            <Label Name="lblBLTo" Content="{Binding ToLable}" Grid.Column="5" Grid.Row="0" Width="28" VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" Foreground="{StaticResource SearchBarCaption}"></Label>

