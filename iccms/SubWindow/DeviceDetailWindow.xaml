                        <Label Name="lblGSMCarrierTworfEnable" Grid.Row="12" Grid.Column="1" Content="射频使能" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierTworfEnable" Grid.Row="12" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding RfEnable}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierTworfFreq" Grid.Row="13" Grid.Column="1" Content="频点" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierTworfFreq" Grid.Row="13" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding RfFreq}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>
                    </Grid>
                </TabItem>

                <!--WCDMA-->
