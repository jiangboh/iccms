                        <TextBox Name="txtGSMDeviceName" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding DomainFullPathName}" ToolTip="{Binding DomainFullPathName}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <!--载波(1)-->
                        <Label Name="lblGSMCarrierOneCaption" Grid.Row="2" Grid.Column="1" Content="[载波(1)]" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right" Foreground="Green" FontWeight="Bold"></Label>

                        <Label Name="lblGSMCarrierOnePLMN" Grid.Row="3" Grid.Column="1" Content="PLMN" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierOnePLMN" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding PLMN}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierOneParaMsPwr" Grid.Row="4" Grid.Column="1" Content="短信类型" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierOneParaMsPwr" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding OpSmsType}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierOneSmsType" Grid.Row="5" Grid.Column="1" Content="侦码方式" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierOneSmsType" Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding OpLuType}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierOnesrfEnable" Grid.Row="6" Grid.Column="1" Content="射频使能" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierOnerfEnable" Grid.Row="6" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding RfEnable}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierOnerfFreq" Grid.Row="7" Grid.Column="1" Content="频点" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierOnerfFreq" Grid.Row="7" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding RfFreq}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>
                        <!--载波(2)-->
                        <Label Name="lblGSMCarrierTwoCaption" Grid.Row="8" Grid.Column="1" Content="[载波(2)]" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right" Foreground="Green" FontWeight="Bold"></Label>

                        <Label Name="lblGSMCarrierTwoPLMN" Grid.Row="9" Grid.Column="1" Content="{Binding PLMNCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierTwoPLMN" Grid.Row="9" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding PLMN}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierTwoParaMsPwr" Grid.Row="10" Grid.Column="1" Content="短信类型" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGSMCarrierTwoParaMsPwr" Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding OpSmsType}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGSMCarrierTwoSmsType" Grid.Row="11" Grid.Column="1" Content="侦码方式" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
