<Window x:Class="iccms.SubWindow.DeviceDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iccms.SubWindow"
        mc:Ignorable="d"
        Icon="..\icon\Safari.ico"
        Name="FrmDeviceDetail"
        Title="{Binding Title}" 
        Height="380" Width="350"
        WindowState="Normal"
        WindowStyle="None"
        AllowsTransparency="True"
        ResizeMode="NoResize"
        Background="{x:Null}"
        Loaded="FrmDeviceDetail_Loaded"
        Closed="FrmDeviceDetail_Closed" 
        MouseMove="FrmDeviceDetail_MouseMove" 
        MouseLeftButtonDown="FrmDeviceDetail_MouseLeftButtonDown" 
        KeyDown="FrmDeviceDetail_KeyDown">

    <Border Width="Auto" Height="Auto" Background="{StaticResource WindowBrush}" BorderThickness="0" CornerRadius="10,10,10,10">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10"></ColumnDefinition>
                <ColumnDefinition Width="95"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition Width="10"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10"></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="10"></RowDefinition>
            </Grid.RowDefinitions>

            <TabControl Name="tabDeviceDetailControl" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Background="Transparent">
                <TabControl.ItemContainerStyle>
                    <Style>
                        <Setter Property="TabItem.Visibility" Value="Collapsed"></Setter>
                    </Style>
                </TabControl.ItemContainerStyle>

                <!--LTE详细信息-->
                <TabItem Name="tabItemLTEDetail">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="10"></ColumnDefinition>
                            <ColumnDefinition Width="95"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition Width="10"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="10"></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="15"></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="10"></RowDefinition>
                        </Grid.RowDefinitions>

                        <Label Name="lblDeviceName" Grid.Row="1" Grid.Column="1" Content="{Binding DeviceNameCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtDeviceName" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding DeviceNameInfo}" ToolTip="{Binding DeviceNameInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblPLMN" Grid.Row="2" Grid.Column="1" Content="{Binding PLMNCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtPLMN" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding PLMNInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblfreqPCI" Grid.Row="3" Grid.Column="1" Content="{Binding FreqPCICaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtfreqPCI" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding FreqPCIInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblcellStatus" Grid.Row="4" Grid.Column="1" Content="{Binding CellStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtcellStatus" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding CellStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblscannerStatus" Grid.Row="5" Grid.Column="1" Content="{Binding ScannerStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtscannerStatus" Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding ScannerStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGPSStatus" Grid.Row="6" Grid.Column="1" Content="{Binding GPSStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGPSStatus" Grid.Row="6" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding GPSStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblGPSDetail" Grid.Row="7" Grid.Column="1" Content="{Binding GPSDetailCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtGPSDetail" Grid.Row="7" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding GPSDetailInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblSyncStatus" Grid.Row="8" Grid.Column="1" Content="{Binding SyncStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtSyncStatus" Grid.Row="8" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding SyncStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblWhiteListSellLearningStatus" Grid.Row="9" Grid.Column="1" Content="{Binding WhiteListSellLearningStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtWhiteListSellLearningStatus" Grid.Row="9" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding Command}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblSyncSource" Grid.Row="10" Grid.Column="1" Content="{Binding SyncSourceCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtSyncSource" Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding SyncSourceInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblAPReadySt" Grid.Row="11" Grid.Column="1" Content="{Binding APReadyStCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtAPReadySt" Grid.Row="11" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding ApReadySt}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblLicenseStatus" Grid.Row="12" Grid.Column="1" Content="{Binding LicenseStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtLicenseStatus" Grid.Row="12" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding LicenseStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblVersion" Grid.Row="13" Grid.Column="1" Content="{Binding VersionCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtVersion" Grid.Row="13" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding VersionInfo}" ToolTip="{Binding VersionInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>
                    </Grid>
                </TabItem>

                <!--GSM详细信息-->
