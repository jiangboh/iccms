                        <Label Name="lblWCDMAcellStatus" Grid.Row="4" Grid.Column="1" Content="{Binding CellStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
                        <TextBox Name="txtWCDMAcellStatus" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding CellStatusInfo}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True"></TextBox>

                        <Label Name="lblWCDMAscannerStatus" Grid.Row="5" Grid.Column="1" Content="{Binding ScannerStatusCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
