                        <Label Name="lblCDMADeviceName" Grid.Row="1" Grid.Column="1" Content="{Binding DeviceNameCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
