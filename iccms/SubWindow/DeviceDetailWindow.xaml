                        <Label Name="lblWCDMAVersion" Grid.Row="10" Grid.Column="1" Content="{Binding VersionCaption}" VerticalAlignment="Center" VerticalContentAlignment="Center" Height="30" HorizontalAlignment="Right"></Label>
