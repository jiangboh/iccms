<Window x:Class="iccms.SubWindow.RedirectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iccms.SubWindow"
        mc:Ignorable="d"
        Icon="..\icon\Safari.ico"
        Name="RedirectParam"
        Title="用户类型" 
        Height="70" Width="100"
        WindowState="Normal"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="{StaticResource WindowBrushSubDialog}"
        Loaded="RedirectParam_Loaded">
    <Grid>
        <DockPanel>
            <StackPanel Orientation="Vertical">
                <CheckBox x:Name="chkWhite" Content="白名单" Margin="0 1" Checked="chkBlack_Checked"/>
                <CheckBox x:Name="chkBlack" Content="黑名单" Margin="0 1" Checked="chkBlack_Checked"/>
                <CheckBox x:Name="chkOther" Content="其它" Margin="0 1" Checked="chkBlack_Checked"/>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
